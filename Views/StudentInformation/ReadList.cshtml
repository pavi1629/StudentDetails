@model IEnumerable<SampleCore.Core.Model.StudentDetail>

@{
    ViewData["Title"] = "Home Page";
}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

<h3 align="center">
    <i class="fa fa-info-circle" style="font-size:24px"></i>
    Students Information  List
</h3>

<style>
    .design {
        border-radius: 25px;
        background-color: lightpink;
        float: right;
    }
    a {
        text-decoration: none;
    }
    body {
        background-image: url('data:image/jpeg;base64,');
        background-size: cover;
    }
</style>
<button class="design">@Html.ActionLink("Create New","Insert")</button>
<br />
<br />
<table class="table table-bordered table-striped table-light table-hover ">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model =>model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Department)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Age)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Class)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JoiningYear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>
            <th>

            </th>
        </tr>
        @foreach (var Student_detail in Model)
        {
            //  @if(Student_detail != null)
            <tr>
                <td>

                    @Html.DisplayFor(model => Student_detail.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(model =>Student_detail.LastName)
                </td>
                <td>
                    @Html.DisplayFor(model => Student_detail.Department)
                </td>
                <td>
                    @Html.DisplayFor(model => Student_detail.Age)
                </td>
                <td>
                    @Html.DisplayFor(model => Student_detail.Class)
                </td>
                <td>
                    @Html.DisplayFor(model => Student_detail.Gender)
                </td>
                <td>
                    @Html.DisplayFor(model => Student_detail.JoiningYear)
                </td>
                <td>
                    @Html.DisplayFor(model => Student_detail.Location)
                </td>

                <td>
                    <button style="border:none" type="button">
                        @Html.ActionLink("", "Edit", new {Student_id =Student_detail.StudentId},
                    new{@class="fa fa-edit",title="Edit StudentDetails",style="color:black;text-decoration:none"})
                    </button>
                    <button style="border:none" type="button" onclick="return confirm('Are you sure want to delete this record?')">
                        @Html.ActionLink("", "Delete", new {Student_id = Student_detail.StudentId }, new{@class="fa fa-trash-o",title="Delete Student Records",
                    style="background:white;border:none;color:black;text-decoration:none" })
                    </button>
                </td>
            </tr>
        }
    </thead>
</table>








